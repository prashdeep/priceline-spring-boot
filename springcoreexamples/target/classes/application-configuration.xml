<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="merchant" class="com.priceline.core.Merchant">
        <constructor-arg name="paymentGateway" ref="amazonPay"/>
    </bean>

    <bean id="amazonPay" class="com.priceline.core.service.AmazonPay"/>
    <bean id="amazonNewPay" class="com.priceline.core.service.AmazonPay"/>

</beans>